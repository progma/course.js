{
    "load": [
        "raphael-min.js",
        "turtle.js"
    ],
    "slides": [
        {
            "name": "intro",
            "type": "html",
            "source": "intro.html",
            "next": "desc1"
        },
        {
            "name": "desc1",
            "type": "html",
            "source": "desc.html",
            "next": "code1 screen1"
        },
        {
            "name": "code1",
            "type": "code",
            "defaultCode": "domecek.js",
            "run": "turtle.run",
            "drawTo": "screen1"
        },
        {
            "name": "screen1",
            "type": "html",
            "source": "screen.html",
            "icon": "turtleIcon.png",
            "next": "wellDone"
        },
        {
            "name": "wellDone",
            "type": "html",
            "source": "wellDone.html"
        }
    ]
}
